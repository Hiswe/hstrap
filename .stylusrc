{
  "borderNone": true,
  "brackets": false,
  "colons": false,
  "commaSpace": true,
  "commentSpace": true,
  "cssLiteral": true,
  "depthLimit": 3,
  "efficient": true,
  "enforceVarStyle": true,
  "enforceBlockStyle": false,
  "extendPref": false,
  "indentSpaces": 2,
  "leadingZero": true,
  "maxWarnings": 50,
  "mixed": true,
  "namingConvention": "lowercase-dash",
  "parenSpace": false,
  "placeholders": true,
  "semicolons": false,
  "trailingWhitespace": true,
  "universal": false,
  "valid": false,
  "zeroUnits": true,
  "zIndexDuplicates": false,
  "zIndexNormalize": false
}